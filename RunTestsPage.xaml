<Page x:Class="Dart.RunTestsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Dart"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RunTestsPage">

    <StackPanel>
        <Label Style="{StaticResource headerTextStyle}"
               HorizontalAlignment="Center">Run settings loaded successfully</Label>
        <Label Style="{StaticResource headerTextStyle}"
               HorizontalAlignment="Center">run your tests?</Label>

        <Button x:Name="runTestsButton" Content="run!"
                Style="{StaticResource buttonStyle}"
                Click="RunTestsButtonClick"/>
    </StackPanel>
</Page>
